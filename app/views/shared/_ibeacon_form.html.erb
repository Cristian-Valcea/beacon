<%= form_for(@ibeacon) do |f| %>
	<%= render 'shared/error_messages', object: f.object %>
	<div class="field">
		<%= f.text_field :beacon_id, placeholder: "Add new ibeacon..." %>
	</div>

	  <p>
		Add logo
	    <%= f.file_field :logo %>
	  </p> 	
	
	  <p>
		Add Image
	    <%= f.file_field :image1 %>
	  </p> 	
	
	
	<%= f.submit "Add", class: "btn btn-large btn-primary" %>
<% end %>